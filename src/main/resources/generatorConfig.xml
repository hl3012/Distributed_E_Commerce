<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator
Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="testTables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/online_shopping? serverTimezone=UTC" userId="root"
                        password="12345678"></jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver> <!-- targetProject: location to generate PO classes -->

        <javaModelGenerator targetPackage="com.kk.onlineshopping.db.po"
                            targetProject="./src/main/java"> <!-- enableSubPackages: whether to use schema as subpackage -->
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/> <!-- trim leading and trailing spaces from database string values -->
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mappers"
                         targetProject="./src/main/resources"> <!-- enableSubPackages: whether to use schema as subpackage -->
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.kk.onlineshopping.db.mappers"
                             targetProject="./src/main/java"> <!-- enableSubPackages: whether to use schema as subpackage -->
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table schema="" tableName="online_shopping_user" domainObjectName="OnlineShoppingUser"
               enableCountByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false"
               selectByExampleQueryId="false"></table>
        <table schema="" tableName="online_shopping_commodity" domainObjectName="OnlineShoppingCommodity"
               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false" selectByExampleQueryId="false"></table>
        <table schema="" tableName="online_shopping_order" domainObjectName="OnlineShoppingOrder"
               enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               enableUpdateByExample="false" selectByExampleQueryId="false"></table>
    </context>
</generatorConfiguration>